<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Downloads the latest version of IPSecuritas.</string>
	<key>Identifier</key>
	<string>com.github.jpiel.download.IPSecuritas</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>IPSecuritas</string>
		<key>SEARCH_PATTERN</key>
		<string>IPSecuritas.*([0-9]+\.[0-9]+)\.dmg</string>
		<key>SEARCH_URL</key>
		<string>http://lobotomo.com/products/IPSecuritas/index.html</string>
		<key>DL_URL</key>
		<string>http://lobotomo.com/products/downloads</string>
	</dict>
	<key>Process</key>
	<array>
		<dict>
			<key>Processor</key>
			<string>URLTextSearcher</string>
			<key>Arguments</key>
			<dict>
				<key>re_pattern</key>
				<string>%SEARCH_PATTERN%</string>
				<key>url</key>
				<string>%SEARCH_URL%</string>
			</dict>
		</dict>
		<dict>
			<key>Processor</key>
			<string>URLDownloader</string>
			<key>Arguments</key>
			<dict>
				<key>filename</key>
				<string>%NAME%_%match%.dmg</string>
				<key>url</key>
				<string>%DL_URL%/%NAME%%20%match%.dmg</string>
			</dict>
		</dict>
		<dict>
			<key>Processor</key>
			<string>EndOfCheckPhase</string>
		</dict>
	</array>
</dict>
</plist>
