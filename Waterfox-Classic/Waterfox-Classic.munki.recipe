<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Description</key>
	<string>Alternative to Firefox.
	This recipe install Classic version of Waterfox.
	To download the Current version of the app, use the Waterfox recipe.
	Waterfox is a fork of Firefox.</string>
	<key>Identifier</key>
	<string>com.github.jpiel.munki.Waterfox-Classic</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>Waterfox-Classic</string>
		<key>MUNKI_CATEGORY</key>
		<string>Utilities</string>
	    <key>APPNAME</key>
	    <string>Waterfox Classic.app</string>
		<key>MUNKI_REPO_SUBDIR</key>
		<string>apps</string>
		<key>pkginfo</key>
		<dict>
			<key>catalogs</key>
			<array>
				<string>testing</string>
			</array>
			<key>category</key>
			<string>%MUNKI_CATEGORY%</string>
			<key>description</key>
			<string>Internet Browser</string>
			<key>developer</key>
			<string>Waterfox</string>
			<key>display_name</key>
			<string>Waterfox</string>
			<key>name</key>
			<string>%NAME%</string>
			<key>unattended_install</key>
			<true/>
			<key>blocking_applications</key>
            <array>
                <string>%APPNAME%</string>
            </array>
		</dict>
	</dict>
	<key>ParentRecipe</key>
	<string>com.github.jpiel.download.Waterfox-Classic</string>
	<key>Process</key>
	<array>
		<dict>
			<key>Processor</key>
			<string>AppDmgVersioner</string>
			<key>Arguments</key>
			<dict>
				<key>dmg_path</key>
				<string>%pathname%</string>
			</dict>
		</dict>
		<dict>
			<key>Processor</key>
			<string>MunkiImporter</string>
			<key>Arguments</key>
			<dict>
				<key>repo_subdirectory</key>
				<string>%MUNKI_REPO_SUBDIR%</string>
				<key>pkg_path</key>
				<string>%pathname%</string>
				<key>CFBundleVersion</key>
				<string>%version%.%WFsmallversion%</string>
			</dict>
		</dict>
	</array>
</dict>
</plist>
